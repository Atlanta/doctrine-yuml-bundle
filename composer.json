{
    "name":                 "onurb/doctrine-yuml-bundle",
    "description":          "Symfony3 Bundle to view Doctrine mapping with Yuml",
    "keywords":             ["yuml", "mapping", "doctrine", "diagram","graph", "uml","bundle","symfony"],
    "type":                 "symfony-bundle",
    "license":              "MIT",
    "minimum-stability":    "dev",
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com"
        },
        {
            "name":     "Bruno Heron",
            "email":    "herobrun@gmail.com"
        },
        {
            "name":     "Symfony Community",
            "homepage": "http://symfony.com/contributors"
        },
        {
            "name":     "Doctrine Project",
            "homepage": "http://www.doctrine-project.org/"
        }
    ],
    "require": {
        "php":                              ">=5.3.2",
        "doctrine/common":                  "~2.0",
        "doctrine/orm":                     "~2.0",
        "onurb/doctrine-metadata-grapher":  "~1.0"
    },
    "require-dev": {
      "symfony/symfony":                                  "~3.0",
      "symfony/monolog-bundle":                           "^2.8",
      "sensio/framework-extra-bundle":                    "^3.0.2",
      "sensio/generator-bundle":                          "^3.0",
      "symfony/phpunit-bridge":                           "^3.0",
      "squizlabs/php_codesniffer":                        "~2",
      "doctrine/doctrine-bundle":                         "~1.5",
      "phpunit/phpunit":                                  "~4.0"
    },
    "autoload": {
      "psr-4":    { "Onurb\\Bundle\\YumlBundle\\":"lib/Onurb/Bundle/YumlBundle/" }
    }
}
